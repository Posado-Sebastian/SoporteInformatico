        "def eliminarEspacios(lista):\n",
        "  for i in range(len(lista)):\n",
        "    lista[i] = lista[i].replace(\" \", \"\")\n",
        "  return lista\n",
        "\n",
        "lista = []\n",
        "for i in range(3):\n",
        "  palabra = input(\"Ingrese palabras \")\n",
        "  lista.append(palabra)\n",
        "\n",
        "lista=eliminarEspacios(lista)\n",
        "print(lista)\n",
